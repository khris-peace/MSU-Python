        "import itertools\n",
        "\n",
        "B = (False, True)\n",
        "solutions=[]\n",
        "\n",
        "for x1, x2, x3 in itertools.product(B,B,B):\n",
        "  if ((not x1 and x2 and not x3) or (not x1 and x2 and x3) or (x1 and not x2 and not x3)) == True:\n",
        "    solutions.append([x1,x2,x3])\n",
        "        \n",
        "for sol in sorted(solutions):\n",
        "\tprint(*sol)"
      ]
