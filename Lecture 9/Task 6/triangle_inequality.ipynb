      "source": [
        "with open('input.csv', mode='r') as f:\n",
        "    txt=f.readlines()[1:]\n",
        "s=0\n",
        "for line in txt:\n",
        "    a,b,c=map(int, line.split(','))\n",
        "    a,b,c = sorted([a,b,c])\n",
        "    s+=a+b>c\n",
        "print(s)"
      ]
    }
  ]
}