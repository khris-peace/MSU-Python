      "source": [
        "with open ('poe_unpublished.txt', 'r') as f:\n",
        "  a = f.read().splitlines()\n",
        "\n",
        "b = sorted(a, key=lambda line: len(line.split()))\n",
        "\n",
        "with open ('poe_decode_attempt.txt', 'w') as f:\n",
        "  for line in b:\n",
        "    words = line.split()\n",
        "    words.sort(key=len)\n",
        "    print(*words, file=f)"
      ]
    }
  ]
