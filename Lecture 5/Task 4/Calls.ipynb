      "source": [
        "def split_line(x):\n",
        "    s = x.split()\n",
        "    return (s[2], -int(s[1]))\n",
        " \n",
        " \n",
        "with open('the_calls.txt', 'r') as f:\n",
        "    lines = f.read().splitlines()\n",
        "    a = sorted(lines, key=split_line)\n",
        " \n",
        "with open('calls.txt', 'w', encoding='utf-8') as f:\n",
        "    f.write('\\n'.join(a))"
      ]
    }
  ]
