      "source": [
        "with open('med_research.txt', 'r') as f:\n",
        "    m = [s.split() for s in f.readlines()]\n",
        "with open('output.txt', 'w') as f:\n",
        "    for i in range(len(m[0])):\n",
        "        s = ''\n",
        "        for j in range(len(m)):\n",
        "            s += f'{m[j][i]} '\n",
        "        f.write(s + '\\n')"
      ]
    }
  ]
}